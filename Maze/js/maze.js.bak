//velikost playerja
var playerWidth = 10;
var playerHeight = 10;
////pozicija
var x = 154;
var y = 10;
//player
var player = new Image();
player.src="images/bala.png";
player.height = "20px";
player.width = "20px";


function move() {
    ctx.drawImage(player, x - playerWidth, y - playerHeight, playerWidth, playerHeight);
}
move()
document.addEventListener("keydown", function (event) {
    switch (event.keyCode) {
        case 37: // left arrow
            x -= 5;
            break;
        case 38: // up arrow
            y -= 5;
            break;
        case 39: // right arrow
            x += 5;
            break;
        case 40: // down arrow
            y += 5;
            break;
    }
    //the left limit and below limit
    if(x>=320){
        x = 319;
    }
    if(y>=322){
        y = 321;
    }
    if(ctx.isPointInStroke(x,y)){
        console.log("hit a limit")
    }else{
        console.log("clear")
    }
    // Limit the player's movement to the canvas size
    x = Math.max(playerWidth, Math.min(canvas.width , x));
    y = Math.max(playerHeight, Math.min(canvas.height , y));
    console.log("coords: "+x+" , "+y)
    // Clear the canvas and redraw the player at the new position
    //ctx.clearRect(0, 0, canvas.width, canvas.height);
    //ctx.clearRect()
    ctx.drawImage(player, x, y - playerHeight, playerWidth, playerHeight);
  });
  
